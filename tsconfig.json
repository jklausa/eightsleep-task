{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "#components/*": ["components/*"],
      "#constants/*": ["constants/*"],
      "#hooks/*": ["hooks/*"],
      "#screens/*": ["screens/*"],
      "#types/*": ["types/*"]
    }
  }
}
